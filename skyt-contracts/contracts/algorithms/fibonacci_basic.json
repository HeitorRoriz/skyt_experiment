{
  "$schema": "../../schema/contract.schema.json",
  "id": "fibonacci_basic",
  "version": "1.0.0",
  "task_intent": "Generate nth Fibonacci number using iteration",
  "prompt": "Write a Python function called 'fibonacci' that takes an integer n and returns the nth Fibonacci number. Use iteration, not recursion.",
  "description": "Basic iterative Fibonacci implementation. The Fibonacci sequence is defined as F(0)=0, F(1)=1, F(n)=F(n-1)+F(n-2) for n>1.",
  "algorithm_family": "fibonacci",
  "constraints": {
    "function_name": "fibonacci",
    "algorithm": "fibonacci sequence",
    "algorithm_variant": "iterative",
    "implementation_style": "iterative",
    "output_type": "integer",
    "variable_naming": {
      "fixed_variables": ["n"],
      "flexible_variables": ["a", "b", "i", "prev", "curr", "next", "temp"],
      "naming_policy": "strict"
    }
  },
  "domain": {
    "inputs": [
      {
        "name": "n",
        "type": "int",
        "constraint": "n >= 0"
      }
    ]
  },
  "out_of_domain": {
    "policy": "must_return",
    "return_value": 0,
    "examples": [
      {"n": -1},
      {"n": -5}
    ]
  },
  "oracle_requirements": {
    "test_cases": [
      {"input": 0, "expected": 0, "description": "Base case F(0)"},
      {"input": 1, "expected": 1, "description": "Base case F(1)"},
      {"input": 2, "expected": 1, "description": "F(2) = F(1) + F(0)"},
      {"input": 5, "expected": 5, "description": "F(5)"},
      {"input": 10, "expected": 55, "description": "F(10)"},
      {"input": 20, "expected": 6765, "description": "F(20) - larger value"}
    ],
    "correctness_threshold": 1.0,
    "required_pass_rate": 0.8
  },
  "normalization_rules": {
    "remove_comments": true,
    "normalize_whitespace": true,
    "enforce_function_name": true
  },
  "rescue_bounds": {
    "allow_function_rename": true,
    "allow_print_removal": true,
    "max_transformations": 5
  },
  "metadata": {
    "author": "skyt_team",
    "created_at": "2025-11-25T00:00:00Z",
    "tags": ["beginner", "dynamic-programming", "sequences"]
  }
}
