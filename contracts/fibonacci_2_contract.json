        "description": "Single element case"
      },
      {
        "name": "two_elements",
        "input": 2,
        "expected_output": [0, 1],
        "description": "Two element case"
      }
    ],
    "edge_cases": [
      {
        "name": "zero_input",
        "input": 0,
        "expected_output": [],
